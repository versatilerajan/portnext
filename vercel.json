{
  "version": 2,
  "builds": [
    {
      "src": "pages/**/*",
      "use": "@vercel/next"
    },
    {
      "src": "components/**/*",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_SPLINE_API": "https://prod.spline.design"
  }
}
