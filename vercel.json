{
  "version": 2,
  "name": "rajan-portfolio",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "app/api/**/*.js": {
      "maxDuration": 10
    }
  },
  "regions": ["bom1", "sin1"],
  "framework": "nextjs"
}
